<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MaterialShapeDrawableView">
        <!-- shapeAppearance is defined in material library. -->
        <attr name="shapeAppearance" />
        <!-- shapeAppearanceOverlay is defined in material library. -->
        <attr name="shapeAppearanceOverlay" />

        <!-- the width of the stroke. -->
        <attr name="msd_strokeWidth" format="dimension" />

        <!-- could be a single color or a color-state-list. -->
        <attr name="msd_strokeColor" format="color|reference" />
        <!-- should a single color. -->
        <attr name="msd_strokeColor_disabled" format="color|reference" />
        <!-- should a single color. -->
        <attr name="msd_strokeColor_focused" format="color|reference" />
        <!-- should a single color. -->
        <attr name="msd_strokeColor_checked" format="color|reference" />
        <!-- should a single color. -->
        <attr name="msd_strokeColor_selected" format="color|reference" />
        <!-- should a single color. -->
        <attr name="msd_strokeColor_pressed" format="color|reference" />
        <!-- should a single color. -->
        <attr name="msd_strokeColor_normal" format="color|reference" />

        <!-- could be a single color or a color-state-list. -->
        <attr name="msd_backgroundColor" format="color|reference" />

        <!-- should a single color. -->
        <attr name="msd_backgroundColor_disabled" format="color|reference" />
        <!-- should a single color. -->
        <attr name="msd_backgroundColor_focused" format="color|reference" />
        <!-- should a single color. -->
        <attr name="msd_backgroundColor_checked" format="color|reference" />
        <!-- should a single color. -->
        <attr name="msd_backgroundColor_selected" format="color|reference" />
        <!-- should a single color. -->
        <attr name="msd_backgroundColor_pressed" format="color|reference" />
        <!-- should a single color. -->
        <attr name="msd_backgroundColor_normal" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="ShapeableView">
        <attr name="msd_selected" format="boolean|reference" />
        <attr name="msd_clip_to_outline" format="boolean|reference" />

        <attr name="shapeAppearance" />
        <attr name="shapeAppearanceOverlay" />

        <attr name="msd_strokeWidth" />
        <attr name="msd_strokeColor" />
        <attr name="msd_strokeColor_disabled" />
        <attr name="msd_strokeColor_focused" />
        <attr name="msd_strokeColor_checked" />
        <attr name="msd_strokeColor_selected" />
        <attr name="msd_strokeColor_pressed" />
        <attr name="msd_strokeColor_normal" />

        <attr name="msd_backgroundColor" />
        <attr name="msd_backgroundColor_disabled" />
        <attr name="msd_backgroundColor_focused" />
        <attr name="msd_backgroundColor_checked" />
        <attr name="msd_backgroundColor_selected" />
        <attr name="msd_backgroundColor_pressed" />
        <attr name="msd_backgroundColor_normal" />

        <attr name="msd_ripple_color" format="color|reference" />
        <!-- default is content -->
        <attr name="msd_ripple_type" format="enum">
            <enum name="content" value="1" />
            <enum name="mask" value="2" />
            <enum name="none" value="3" />
        </attr>
    </declare-styleable>

